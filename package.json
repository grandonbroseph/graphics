{
  "name": "graphics",
  "version": "0.0.2",
  "description": "Powerful JavaScript graphics library for use in the browser.",
  "author": "Brandon Semilla (https://github.com/semibran)",
  "repository": {
    "type": "git",
    "url": "https://github.com/semibran/graphics.git"
  },
  "scripts": {
    "start": "npm-run-all --parallel serve watch",
    "serve": "browser-sync start --server --no-ui --serveStatic docs --files docs",
    "watch": "watchify src/index.js -o docs/graphics.js -v"
  },
  "license": "MIT",
  "devDependencies": {
    "browser-sync": "^2.18.5",
    "npm-run-all": "^3.1.2",
    "watchify": "^3.8.0"
  }
}
